{
  "name": "HeroesOfTheStorm_TryMode2.0Installer",
  "version": "0.0.1",
  "description": "Installer for Heroes of the Storm - Try Mode 2.0",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build:windows": "electron-builder -w --publish never",
    "build:linux": "electron-builder -l --publish never",
    "build:macos": "electron-builder -m --publish never",
    "start": "electron src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jamiephan/HeroesOfTheStorm_TryMode2.0Installer.git"
  },
  "keywords": [],
  "author": "Jamie Phan",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jamiephan/HeroesOfTheStorm_TryMode2.0Installer/issues"
  },
  "homepage": "https://github.com/jamiephan/HeroesOfTheStorm_TryMode2.0Installer#readme",
  "devDependencies": {
    "electron": "^13.1.4",
    "electron-builder": "^22.11.7"
  },
  "dependencies": {
    "mkdirp": "^1.0.4"
  },
  "build": {
    "appId": "jamiephan.TryMode20.Installer",
    "productName": "Try Mode 2.0 Installer",
    "artifactName": "TryMode2.0Installer-${version}-${os}_${arch}.${ext}",
    "win": {
      "target": [
        {
          "target": "nsis"
        },
        {
          "target": "zip"
        },
        {
          "target": "portable"
        }
      ]
    },
    "nsis": {
      "artifactName": "TryMode2.0InstallerSetup-${version}-${os}_${arch}.${ext}"
    }
  }
}